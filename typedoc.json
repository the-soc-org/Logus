{
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "plugin": ["typedoc-plugin-missing-exports", "typedoc-theme-oxide"],
  "exclude": [
    "**/.github/**",
    "**/.vscode/**",
    "**/lib/**",
    "**/node_modules/**",
    "**/scripts/**",
    "**/test/**"
  ],
  "excludePrivate": false,
  "excludePrivateClassFields": false,
  "excludeProtected": false,
  "excludeNotDocumented": false,
  "readme": "README.md",
  "name": "Logus",
  "theme": "oxide"
}
